<view class='container'>
  <view>
    <button type='primary' bindtap='loadData'>加载数据</button>
  </view>

  <view class='newsitem' wx:for='{{newsdata}}' wx:for-item='item' wx:key='index'>
    <image style='width:300px; height:200px;' src='{{item.urlToImage}}'></image>
    "{{item.title}}"
  </view>
</view>